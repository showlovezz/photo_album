<br>
<table class="table table-striped table-ondensed">
	<% @photos.each do |photo| %>
		<tr>
			<td><%= photo.title %></td>
			<td><%= link_to 'Show', photo_path(photo) %></td>
			<td><%= link_to 'Edit', edit_photo_path(photo) %></td>
			<td><%= link_to 'Delete', photo_path(photo), method: :delete, data:{confirm: "Are you sure ?"} %></td>
		</tr>
	<% end %>	
</table>

<%= link_to 'New Photo', new_photo_path %>